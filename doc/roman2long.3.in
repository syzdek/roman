.TH "ROMAN NUMERAL" 3 "@RELEASE_MONTH@" "@PACKAGE_TARNAME@ @PACKAGE_VERSION@"
.SH NAME
roman2long \- convert ASCII string containing a Roman Numeral value to
.I signed int.
.SH LIBRARY
Roman Numeral Conversion API (libroman, -lroman)
.SH SYNOPSIS
\fB#include <roman.h>\fR
.sp
\fIint\fR
.br
\fBroman2long\fR(\fIconst char * str\fR);
.SH DESCRIPTION
The function
.B roman2long
takes an argument of
.I str
and returns an integer containing the value of the Roman Numeral.
.\"
.\"The
.\".B roman2long()
.\"function converts
.\".I numeral
.\"from a
.\".I string
.\"to a 
.\".I int
.\"which contains the value of the Roman numeral.
.SH RETURN VALUES
If successful, 
.B roman2long
returns the numeric value of
.I str
or \-1 if an error occurred (in which case,
.I errno
is set appropriately).
.SH ERRORS
.TP
.B EFAULT
bad address in \fIstr\fR.
.TP
.B EINVAL
\fIstr\fR contains either invalid characters or an invalid Roman numeral.
.SH EXAMPLES
\fB#include <stdio.h>\fB
.br
\fB#include <roman.h>\fB
.sp
\fBint main\fR(\fIint argc\fR, \fIchar * argv[]\fR);
.sp
\fBint main\fR(\fIint argc\fR, \fIchar * argv[]\fR)
.br
{
.br
   \fIint i\fR;
   \fIint num\fR;
.sp
   \fBfor\fR(\fIi\fR = 1; \fIi\fR < \fIargc\fR; \fIi\fR++)
.br
   {
.br
      \fInum\fR = \fBroman2long\fR(\fIargv\fR[\fIi\fR]);
.br
      \fBif\fR(\fInum\fR == \fI-1\fR)
.br
         \fBperror\fR("roman2long()");
.br
      \fBelse\fR
.br
         \fBprintf\fR("%s == %i\\n", \fIargv[i]\fR, \fInum\fR);
.br
   }
.sp
   \fBreturn\fR(\fI0\fR);
.br
}
